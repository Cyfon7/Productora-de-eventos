<h1>Concerts</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Group</th>
            <th>Attendants</th>
            <th>Date</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @concerts.each do |concert| %>
            <tr>
                <td><%= concert.concert_title %></td>
                <td><%= concert.group %></td>
                <td><%= concert.num_of_attendants %></td>
                <td><%= concert.concert_date %></td>
                <td><%= link_to 'Show', concert %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<%= link_to 'New Concert', new_concert_path %>

